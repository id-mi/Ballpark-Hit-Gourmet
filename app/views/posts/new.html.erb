<h1>新規投稿</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>グルメ名</h4>
    <%= f.text_field :gourme_name %>
  <h4>店名</h4>
    <%= f.text_field :shop_name %>  
  <h4>価格（税込）</h4>
    <%= f.text_field :price %>
  <h4>感想</h4>  
    <%= f.text_area :content %>
  <h4>画像</h4>
    <%= f.file_field :image %>
    <p></p>
  <%= f.submit "投稿する" %>
<% end %>